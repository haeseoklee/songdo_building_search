<%- include('../includes/head.ejs') %>
<link rel="stylesheet" href="/css/buildings.css">

</head>
<body>
    <% if (!buildings.length) { %>
        <h1>현재 분양중인 건물이 없습니다</h1>
    <% } else { %>
        <% buildings.forEach(building => { %>
            <div class="img building">
                <a href="/building/<%= building._id %>">
                    <div class="content">
                        <h4><%= building.name %></h4>
                    </div>
                    <div class="img-cover" style="background-image: url(<%= building.imageUrl %>)"></div>
                </a>
            </div>
        <% }) %>
    <% } %>
    <div class="loader loader-default"></div>
    <script src="/js/buildings.js"></script>
    
<%- include('../includes/end.ejs') %>
